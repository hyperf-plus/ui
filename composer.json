{
  "name": "hyperf-plus/ui",
  "description": "hyperf UI 插件，用php代码写amis组件神器",
  "license": "MIT",
  "authors": [
    {
      "name": "毛自豪",
      "email": "4213509@qq.com"
    }
  ],
  "homepage": "https://hyperf.plus",
  "keywords": [
    "hyperf",
    "ui",
    "amis",
    "Admin",
    "react"
  ],
  "require": {
    "php": ">=8.1",
    "hyperf/macroable": "^3.1",
    "phper666/generate-file": "~1.0"
  },
  "autoload": {
    "psr-4": {
      "HPlus\\UI\\": "src/"
    },
    "files": [
      "../ui/src/Support/helpers.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "HPlus\\UI\\Tests\\": "tests"
    }
  },
  "extra": {
    "hyperf": {
      "config": "HPlus\\UI\\ConfigProvider"
    }
  },
  "scripts": {
    "post-install-cmd": [
      "@php ./bin/hyperf.php ui:init"
    ],
    "post-update-cmd": "HPlus\\UI\\Commands\\initCommand::handle"
  }
}