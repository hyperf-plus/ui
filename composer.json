{
  "name": "hyperf-plus/ui",
  "description": "hyperf UI 插件，用php代码写element组件神器，无需修改vue文件即可快速实现各种复杂的element页面搭建",
  "license": "MIT",
  "authors": [
    {
      "name": "毛自豪",
      "email": "4213509@qq.com"
    }
  ],
  "homepage": "https://hyperf.plus",
  "keywords": [
    "hyperf",
    "ui",
    "element",
    "Admin",
    "Vue"
  ],
  "require": {
    "php": ">=7.3",
    "ext-swoole": ">=4.5",
    "hyperf/http-message": "~2.0.0",
    "hyperf/http-server": "~2.0.0",
    "hyperf/config": "~2.0.0",
    "hyperf/di": "~2.0.1",
    "hyperf/framework": "~2.0.0",
    "mzh/hyperf-helper": "^0.1.7",
    "hyperf/db-connection": "~2.0.0",
    "hyperf/model-cache": "~2.0.0",
    "hyperf/filesystem": "^2.0",
    "mzh/hyperf-validate": "^1.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^8.0"
  },
  "autoload": {
    "psr-4": {
      "HPlus\\UI\\": "src/"
    },
    "files": [
      "src/helpers.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "HPlus\\UI\\Tests\\": "tests"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "HPlus\\UI\\ConfigProvider"
      ],
      "aliases": {
        "UI": "HPlus\\UI\\Facades\\UI"
      }
    }
  },
  "scripts": {
    "post-install-cmd": [
      "@php bin/hyperf.php ui:init"
    ],
    "post-update-cmd": [
      "@php bin/hyperf.php ui:publish"
    ]
  }
}
